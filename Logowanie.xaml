<UserControl x:Class="PZ_Panel_Logowania.Logowanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PZ_Panel_Logowania"
        mc:Ignorable="d"
        Height="450" Width="800" Background="White">
    <Grid>
        
        <Button x:Name="btn_zaloguj" Content="Zaloguj się" HorizontalAlignment="Left" Margin="296,222,0,0" VerticalAlignment="Top" Width="138" Click="btn_zaloguj_Click"/>
        <Label x:Name="Lab_nazwa" Content="Nazwa użytkownika:" HorizontalAlignment="Left" Margin="195,128,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.979,0.771" Width="136"/>
        <PasswordBox x:Name="Txt_haslo" HorizontalAlignment="Left" Margin="348,167,0,0" VerticalAlignment="Top" Width="182" RenderTransformOrigin="0.526,-1.667" Height="23"/>
        <TextBox x:Name="Txt_nazwa" HorizontalAlignment="Left" Height="23" Margin="349,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" RenderTransformOrigin="0.5,0.5" TextChanged="TextBox_TextChanged"/>
        <Label x:Name="Lab_haslo" Content="Hasło:" HorizontalAlignment="Left" Margin="198,166,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.271,0.552" Width="86"/>

    </Grid>
</UserControl>
