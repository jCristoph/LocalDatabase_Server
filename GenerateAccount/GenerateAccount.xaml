<Window x:Class="LocalDatabase_Server.GenerateAccount.GenerateAccount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocalDatabase_Server.GenerateAccount"
        mc:Ignorable="d"
        Title="GenerateAccount" Height="450" Width="800">
    <Grid>
        <TextBox  x:Name="SurnameText" HorizontalAlignment="Left" Margin="299,106,0,0" TextWrapping="Wrap" Text="Surname" VerticalAlignment="Top" Height="30" Width="180"/>
        <TextBox  x:Name="NameText" HorizontalAlignment="Left" Margin="299,159,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Height="30" Width="180"/>
        <Button Content="Create Account" HorizontalAlignment="Left" Margin="312,277,0,0" VerticalAlignment="Top" Width="150" Height="30"/>
    </Grid>
</Window>
